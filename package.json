{
  "name": "rms-forms",
  "version": "1.0.0",
  "description": "Form elements for use on RMS",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/scss/ -o dist/css",
    "build-task:autoprefixer": "postcss dist/css/*.css --use autoprefixer -d dist/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/scss/**/*.scss' -c 'npm run sass:build'",
    "js:build": "babel src/js/ -o dist/js/rms.js",
    "js:concat:vendors": "",
    "js:concat": "",
    "js:babel": "babel dist/js/*.js -o dist/js/rms.js",
    "dev": "npm-run-all -p sass:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josephshambrook/RMS-forms.git"
  },
  "author": "RMS / Joseph Shambrook",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josephshambrook/RMS-forms/issues"
  },
  "homepage": "https://github.com/josephshambrook/RMS-forms#readme",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "bootstrap": "^4.1.3",
    "chokidar-cli": "^1.2.1",
    "concat": "^1.0.3",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "concat": "^1.0.3"
  }
}
